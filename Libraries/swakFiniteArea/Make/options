include $(MAKE_DIR)/../../rules/versionOptions

ifeq "$(WMAKE_NEW_CONVENTION)" "new"
  include $(MAKE_DIR)/mybisonNew
  include $(MAKE_DIR)/myflexNew
else
  include $(MAKE_DIR)/mybison
  include $(MAKE_DIR)/myflex

  sinclude $(MAKE_DIR)/mydependencies
endif

EXE_INC = -I$(OBJECTS_DIR) \
	$(FOAM_DEV_OPTION) 	\
    -I$(LIB_SRC)/finiteVolume/lnInclude \
    -I$(LIB_SRC)/finiteArea/lnInclude \
    -I$(LIB_SRC)/surfMesh/lnInclude \
    -I$(LIB_SRC)/sampling/lnInclude \
    -I../swak4FoamParsers/lnInclude/ \
    -I../groovyBC/lnInclude/ \
    -I../swakFunctionObjects/lnInclude/

LIB_LIBS = -lfiniteVolume \
	-lfiniteArea \
	-lsampling \
	-L$(FOAM_USER_LIBBIN) \
	-lswak4FoamParsers \
        -lswakFunctionObjects
